package com.sys4soft;

public class IniciacaoArrays {

	public static void main(String[] args) {
		
		// array
		String[] alunos = {
			"João",
			"Rui",
			"Francisco",
			"Raquel",
			"Ana",
			"Clara",
			"Carlos"
		};
		
		// System.out.println(alunos[6]);
		
		for(int i = 0; i < alunos.length; i++){
			System.out.println(i);
		}

	}

}
