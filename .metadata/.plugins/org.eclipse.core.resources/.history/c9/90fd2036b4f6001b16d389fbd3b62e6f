package com.sys4soft;

import java.util.Arrays;

public class OrdenandoDados {

	public static void main(String[] args) {

		// array
		String[] alunos = {
			"João",
			"Rui",
			"Francisco",
			"Raquel",
			"Ana",
			"Clara",
			"Carlos"
		};

		Arrays.sort(alunos);
		
		for(String nome:alunos){
			System.out.println(nome);
		}
		
	}

}
