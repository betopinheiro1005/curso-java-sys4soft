package com.sys4soft;

public class TestesOperadoresAtribuicao {

	public static void main(String[] args) {
		int valor = 10;
		
		valor += 50;
		System.out.println(valor);
		
		valor -= 30;
		System.out.println(valor);

		valor *= 2;
		System.out.println(valor);
		
		valor /= 5;
		System.out.println(valor);

		valor %= 7;
		System.out.println(valor);
	}

}
