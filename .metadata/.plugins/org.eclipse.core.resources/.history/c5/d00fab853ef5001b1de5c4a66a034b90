package com.sys4soft;

public class Socio {
	String nome;
	int idade;
	String sexo;
	boolean vip;
	
	// construtor da classe
	public Socio(String nome){
		// define o nome do sócio
		this.nome = nome;
		// System.out.println("Novo sócio criado!");
	}
	
	public void ApresentarSocio(){
		System.out.println("Nome: " + nome);
	}
	
}
